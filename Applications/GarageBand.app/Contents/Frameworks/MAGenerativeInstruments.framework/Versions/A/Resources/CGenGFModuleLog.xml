<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>inputAttributes</key>
	<dict>
		<key>inputValue</key>
		<dict>
			<key>description</key>
			<string>The value to print in the log.</string>
			<key>name</key>
			<string>Value</string>
		</dict>
		<key>inputLogToConsole</key>
		<dict>
			<key>description</key>
			<string>Activates output to console.</string>
			<key>name</key>
			<string>Log To Console</string>
		</dict>		
		<key>inputLogToUI</key>
		<dict>
			<key>description</key>
			<string>Activates output to UI.</string>
			<key>name</key>
			<string>Log To UI</string>
		</dict>		
	</dict>
	<key>nodeAttributes</key>
	<dict>
		<key>description</key>
		<string>Prints the content of the string input to console and/or UI whenever it changes.</string>
		<key>name</key>
		<string>Log</string>
	</dict>
</dict>
</plist>
