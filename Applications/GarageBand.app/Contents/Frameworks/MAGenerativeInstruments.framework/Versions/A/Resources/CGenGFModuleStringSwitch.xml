<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>nodeAttributes</key>
	<dict>
		<key>name</key>
		<string>String Switch</string>
		<key>description</key>
		<string>Switch Patterns depending on a string.</string>
	</dict>
	<key>_portAttributes</key>
	<dict>
		<key>description</key>
		<string>A generic output port.</string>
		<key>name</key>
		<string>Index #%i</string>
	</dict>	
	<key>inputAttributes</key>
	<dict>
		<key>inputPatterns</key>
		<dict>
			<key>description</key>
			<string>vector of patterns</string>
			<key>name</key>
			<string>Patterns</string>
		</dict>
		<key>inputString</key>
		<dict>
			<key>description</key>
			<string>input string</string>
			<key>name</key>
			<string>String</string>
		</dict>
	</dict>
	<key>outputAttributes</key>
	<dict>
		<key>outputDefault</key>
		<dict>
			<key>description</key>
			<string>If string is not found, patterns are sent to Default out.</string>
			<key>name</key>
			<string>Default #0</string>
		</dict>
		<key>outputActiveIndex</key>
		<dict>
			<key>description</key>
			<string>The active index which matched a string. '0' if default is used.</string>
			<key>name</key>
			<string>Active Index</string>
		</dict>		
	</dict>
</dict>
</plist>
