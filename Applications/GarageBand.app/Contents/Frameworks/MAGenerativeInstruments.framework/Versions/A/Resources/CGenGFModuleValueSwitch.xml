<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>nodeAttributes</key>
	<dict>
		<key>name</key>
		<string>Value Switch</string>
		<key>description</key>
		<string>Switch Patterns depending on a value.</string>
	</dict>
	<key>_portAttributes</key>
	<dict>
		<key>description</key>
		<string>A generic output port.</string>
		<key>name</key>
		<string>Index #%i</string>
	</dict>	
	<key>inputAttributes</key>
	<dict>
		<key>inputPatterns</key>
		<dict>
			<key>description</key>
			<string>vector of patterns</string>
			<key>name</key>
			<string>Patterns</string>
		</dict>
		<key>inputValue</key>
		<dict>
			<key>description</key>
			<string>input value</string>
			<key>name</key>
			<string>Value</string>
		</dict>
	</dict>
	<key>outputAttributes</key>
	<dict>
		<key>outputDefault</key>
		<dict>
			<key>description</key>
			<string>If value is not found in ranges, patterns are sent to Default out.</string>
			<key>name</key>
			<string>Default #0</string>
		</dict>
		<key>outputActiveIndex</key>
		<dict>
			<key>description</key>
			<string>The active index which matched a range. '0' if default is used.</string>
			<key>name</key>
			<string>Active Index</string>
		</dict>		
		<key>outputNormalizedValue</key>
		<dict>
			<key>description</key>
			<string>The normalized value of the value in matching range.</string>
			<key>name</key>
			<string>Normalized Value</string>
		</dict>				
	</dict>
</dict>
</plist>
