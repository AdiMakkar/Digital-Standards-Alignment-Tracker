<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>nodeAttributes</key>
	<dict>
		<key>name</key>
		<string>Patterns Merger</string>
		<key>description</key>
		<string>If 'Merge Events' is disabled, merges two vectors of patterns into one vector.
		If 'Merge Events' is enabled, the events of the first patterns in both input vectors are merged into the first pattern of the first input vector.
		</string>
	</dict>
	<key>inputAttributes</key>
	<dict>
		<key>inputPatterns1</key>
		<dict>
			<key>description</key>
			<string>vector of patterns</string>
			<key>name</key>
			<string>Patterns</string>
		</dict>
		<key>inputPatterns2</key>
		<dict>
			<key>description</key>
			<string>vector of patterns</string>
			<key>name</key>
			<string>Patterns</string>
		</dict>
		<key>inputMergeEventsOfFirstPatterns</key>
		<dict>
			<key>description</key>
			<string>Determines if events get merged</string>
			<key>name</key>
			<string>Merge Events</string>
		</dict>
	</dict>
	<key>outputAttributes</key>
	<dict>
		<key>outputPatterns</key>
		<dict>
			<key>description</key>
			<string></string>
			<key>name</key>
			<string>Patterns</string>
		</dict>
	</dict>
</dict>
</plist>
